{
  "name": "@rdu/orchts",
  "version": "0.1.0",
  "type": "module",
  "description": "An orchestration framework for Large Language Models (LLM) with TypeScript support",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "private": false,
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "format": "prettier --write 'src/**/*.ts'",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "llm",
    "ai",
    "orchestration",
    "typescript",
    "openai",
    "agents",
    "function-calling",
    "conversation",
    "decorators",
    "metadata"
  ],
  "author": {
    "name": "Ronny D.",
    "email": "info@netf.de"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rdu/orchts.git"
  },
  "bugs": {
    "url": "https://github.com/rdu/orchts/issues"
  },
  "homepage": "https://github.com/rdu/orchts#readme",
  "dependencies": {
    "chalk": "^5.3.0",
    "logform": "^2.6.1",
    "openai": ">=4.0.0",
    "pino": "^8.16.0",
    "pino-pretty": "^11.3.0",
    "reflect-metadata": ">=0.1.13",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/jest": "^29.5.13",
    "@types/node": "^22.7.6",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@typescript-eslint/parser": "^8.11.0",
    "eslint": "^9.13.0",
    "eslint-plugin-jest": "^28.8.3",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  },
  "peerDependencies": {
    "openai": ">=4.0.0",
    "reflect-metadata": ">=0.1.13",
    "typescript": ">=4.8.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}